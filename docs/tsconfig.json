{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "moduleResolution": "Bundler",
    "types": ["vite/client", "element-plus/global"],
    "paths": {
      "@/*": ["./src/*"],
    },

    "allowJs": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
  },
}
